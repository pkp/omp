msgid "manager.setup.securitySettings.note"
msgstr "Outras opções relacionadas com segurança e acesso podem ser configuradas na página <a href=\"{$accessAndSecurityUrl}\" target=\"_blank\" class=\"action\">Acesso e segurança</a>."

msgid "manager.setup.selectEditorDescription"
