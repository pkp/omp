msgstr "Vælg en skrifttypekombination, der passer til dette tidsskrift."

msgid "plugins.themes.default.option.typography.label"
msgstr "Typografi"

msgid "plugins.themes.default.description"
msgstr "Dette tema implementerer standardtemaet."

msgid "plugins.themes.default.name"
msgstr "Standard-tema"
