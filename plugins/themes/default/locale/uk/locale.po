msgid "plugins.themes.default.name"
msgstr "Тема за замовчуванням"

msgid "plugins.themes.default.description"
msgstr "Ця тема впроваджує тему за замовчуванням."

msgid "plugins.themes.default.option.typography.label"
msgstr "Типографіка"

msgid "plugins.themes.default.option.typography.description"
msgstr "Оберіть комбінацію шрифтів, що відповідає цьому е-видавництву."

msgid "plugins.themes.default.option.typography.notoSans"
msgstr ""
"Noto Sans: екранний шрифт, розроблений Google для широкої мовної підтримки."

msgid "plugins.themes.default.option.typography.notoSerif"
msgstr "Noto Serif: варіант екранного шрифта із засічками від Google."

msgid "plugins.themes.default.option.typography.notoSerif_notoSans"
msgstr ""
"Noto Serif/Noto Sans: Сполучення шрифтів: із засічками для заголовків і "
"рубаний для основного тексту."

msgid "plugins.themes.default.option.typography.notoSans_notoSerif"
msgstr ""
"Noto Sans/Noto Serif: Сполучення шрифтів: рубаний для заголовків та із "
"засічками для основного тексту."

msgid "plugins.themes.default.option.typography.lato"
msgstr "Lato: Популярний сучасний рубаний шрифт."

msgid "plugins.themes.default.option.typography.lora"
msgstr "Lora: Широкий шрифт із засічками для зручного онлайнового читання."

msgid "plugins.themes.default.option.typography.lora_openSans"
msgstr ""
"Lora/Open Sans: Сполучення шрифтів: із засічками для заголовків та рубаний "
"для основного тексту."

msgid "plugins.themes.default.option.colour.label"
msgstr "Колір"

msgid "plugins.themes.default.option.colour.description"
msgstr "Оберіть колір для заголовку."

msgid "plugins.themes.default.option.useHomepageImageAsHeader.label"
msgstr "Фонове зображення заголовка"

msgid "plugins.themes.default.option.useHomepageImageAsHeader.description"
msgstr ""
"Коли зображення головної сторінки вивантажено, показувати його на фоні "
"заголовка замість звичного позиціонування на головній сторінці."

msgid "plugins.themes.default.option.useHomepageImageAsHeader.option"
msgstr "Показувати зображення головної сторінки фоном заголовка."

msgid "plugins.themes.default.option.showCatalogSeriesListing.label"
msgstr "Показати серії"

msgid "plugins.themes.default.option.showCatalogSeriesListing.option"
msgstr ""
"Додати список покликань на всі серії е-видавництва на сторінці каталогу"

msgid "plugins.themes.default.option.displayStats.label"
msgstr "Налаштування відображення статистики використання"

msgid "plugins.themes.default.option.displayStats.none"
msgstr "Не відображати для читача чат статистики використання подань."

msgid "plugins.themes.default.option.displayStats.bar"
msgstr ""
"Використовувати панельний вид чату для відображення статистики використання."

msgid "plugins.themes.default.option.displayStats.line"
msgstr ""
"Використовувати лінійний вид чату для відображення статистики використання."

msgid "plugins.themes.default.displayStats.monthInitials"
msgstr "січ лют берез квіт трав черв лип серп жовт листоп груд"

msgid "plugins.themes.default.displayStats.downloads"
msgstr "Завантаження"

msgid "plugins.themes.default.displayStats.noStats"
msgstr "Дані завантаження поки що не є доступними."

msgid "plugins.themes.default.prevSlide"
msgstr "Попередній слайд"

msgid "plugins.themes.default.nextSlide"
msgstr "Наступний слайд"

msgid "plugins.themes.default.option.showDescriptionInPressIndex.option"
msgstr "Показати загальну інформацію про е-видавництво на головній сторінці."
