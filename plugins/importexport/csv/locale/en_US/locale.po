msgid "plugins.importexport.csv.displayName"
msgstr "CSV Content Import Plugin"

msgid "plugins.importexport.csv.description"
msgstr "Import submissions into presses from CSV data."

msgid "plugins.importexport.csv.cliOnly"
msgstr ""
"\n"
"\t\t<p>This plugin currently supports command-line operation only. "
"Execute...\n"
"\t\t\t<pre>php tools/importExport.php CSVImportExportPlugin</pre>\n"
"\t\t\t...for more information.</p>\n"
"\t"

msgid "plugins.importexport.csv.cliUsage"
msgstr ""
"Command-line tool for importing CSV data into OMP\n"
"\t\t\tUsage:\n"
"\t\t\t{$scriptName} [--dry-run] fileName.csv username\n"
"\t\t\tThe --dry-run option can be used to test without making changes.\n"
"\t\t\tSpecify the username you wish to assign the submissions to.\n"

msgid "plugins.importexport.csv.fileDoesNotExist"
msgstr "The file \"{$filename}\" does not exist.  Exiting."

msgid "plugins.importexport.csv.unknownUser"
msgstr "Unknown User: \"{$username}\".  Exiting."

msgid "plugins.importexport.csv.unknownLocale"
msgstr "Unknown Locale: \"{$locale}\".  Skipping."

msgid "plugins.importexport.csv.unknownPress"
msgstr "Unknown Press: \"{$contextPath}\".  Skipping."

msgid "plugins.importexport.csv.noGenre"
msgstr "There is no {$manuscript} genre."

msgid "plugins.importexport.csv.noAuthorGroup"
msgstr ""
"There is no default author group in the press {$press}.  Skipping this "
"submission."

msgid "plugins.importexport.csv.noSeries"
msgstr ""
"The series path {$seriesPath} does not exist.  Unable to add it to this "
"submission."

msgid "plugins.importexport.csv.import.submission"
msgstr "Submission: \"{$title}\" successfully imported."

msgid "plugins.importexport.csv.import.success.description"
msgstr ""
"The import was successful. Successfully-imported items are listed below."

msgid "plugins.importexport.csv.allCategoriesMustExists"
msgstr "All CSV categories must be already existing on OMP. Please create it manually and retry."

msgid "plugins.importexport.csv.invalidAssetFilename"
msgstr "Invalid asset name for submission \"{$title}\"."

msgid "plugins.importexport.csv.invalidCoverImageFilename"
msgstr "Invalid cover image filename for submission \"{$title}\""

msgid "plugins.importexport.csv.requiredFieldsMissing"
msgstr "Required fields missing. Please verify the required fields for this row."

msgid "plugins.importexport.csv.csvDoesntContainAllFields"
msgstr "This row doesn't contain all fields."

msgid "plugin.importexport.csv.seeInvalidRowsFile"
msgstr "{$processedRows} submissions were imported. {$failedRows} failed. Please see the file \"invalid_rows.csv\" at the same directory as the CSV file."

msgid "plugin.importexport.csv.allDataSuccessfullyImported"
msgstr "{$processedRows} submissions were successfully imported."

msgid "pugin.importexport.csv.missingHeadersOnCsv"
msgstr "The following headers are missing on your CSV file: \"{$missingHeaders}\". Please update it as shown on file sample.csv."

msgid "plugin.importexport.csv.erroWhileSavingBookCoverImage"
msgstr "An error occurred while copying the book cover image."

msgid "plugin.importexport.csv.errorWhileSavingThumbnail"
msgstr "An error occurred while saving book cover image thumbnail."

msgid "plugin.importexport.csv.errorWhileSavingSubmissionFile"
msgstr "An error ocurred while saving the Submission file."

msgid "plugin.importexport.csv.seriesPathNotFound"
msgstr "Could not found a series by path \"{$seriesPath}\"."

msgid "plugins.importexport.csv.invalidDateFormat"
msgstr "Invalid date format for \"{$date}\". Please use YYYY-MM-DD format (e.g., 2024-03-15)."

msgid "plugins.importexport.csv.invalidDate"
msgstr "Invalid date \"{$date}\". Please provide a valid date in YYYY-MM-DD format."
